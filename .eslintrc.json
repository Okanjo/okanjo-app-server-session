{
    "env": {
        "node": true,
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
    ],
    "extends": "eslint:recommended",
    "globals": {
        "require": true,
        "module": true,
        "describe": true,
        "it": true,
        "before": true,
        "after": true,
        "Promise": true
    },
    "overrides": [
        {
            "files": ["docs/**"],
            "rules": {
                "no-console": "off"
            }
        }
    ]
}